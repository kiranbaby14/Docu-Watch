# DocuSign OAuth Configuration
DS_CLIENT_ID=your_docusign_client_id
DS_USER_ID=your_docusign_user_id
DS_CLIENT_SECRET=your_docusign_client_secret
DS_AUTH_SERVER=https://account-d.docusign.com  # Use account.docusign.com for production

# Application Configuration
APP_URL=http://localhost:8000/api
ENVIRONMENT=development  # Options: development, staging, production
CALLBACK_ROUTE=/callback

# Frontend Settings
FRONTEND_URL=http://localhost:3000
FRONTEND_CALLBACK_ROUTE=/dashboard

# Neo4j Database Configuration
NEO4J_URI=your_neo4j_uri
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password
NEO4J_DATABASE=neo4j

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key